{
  "name": "wizards/rest-bundle",
  "description": "Integration of php-rest-api into Symfony.",
  "keywords": [ "rest", "hateoas", "jsonapi", "api" ],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Romain Richard",
      "email": "rrichard.ext@wedigital.garden"
    }
  ],
  "require": {
    "php": ">7",
    "wizards/rest-api": "0.7.0",
    "symfony/http-kernel": "^4.1",
    "symfony/psr-http-message-bridge": "^1.1",
    "zendframework/zend-diactoros": "^1.8",
    "sensio/framework-extra-bundle": "^5.2"
  },
  "require-dev": {
    "symfony/form": "^4.1",
    "phpstan/phpstan": "^0.10.8",
    "squizlabs/php_codesniffer": "^3.4",
    "phpmd/phpmd": "^2.6"
  },
  "autoload": {
    "psr-0": { "Wizards\\RestBundle": "" }
  },
  "target-dir": "Wizards/RestBundle"
}
